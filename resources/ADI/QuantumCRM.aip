nerIdiomaDeSesion]&#13;&#10;    @UsuarioID UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    WITH SesionIdioma AS (&#13;&#10;        SELECT &#13;&#10;            i.idioma_id AS idioma_id,&#13;&#10;            i.nombre AS idioma_nombre&#13;&#10;        FROM &#13;&#10;            sesion s&#13;&#10;            INNER JOIN idioma i ON s.ultimo_idioma = i.idioma_id&#13;&#10;        WHERE &#13;&#10;            s.usuario_id = @UsuarioID&#13;&#10;            AND s.estado = 1  -- Assuming 1 means active session&#13;&#10;        ORDER BY &#13;&#10;            s.fecha_inicio DESC  -- Get the most recent session&#13;&#10;        OFFSET 0 ROWS&#13;&#10;        FETCH NEXT 1 ROW ONLY&#13;&#10;    )&#13;&#10;&#13;&#10;    SELECT idioma_id, idioma_nombre FROM SesionIdioma&#13;&#10;    UNION ALL&#13;&#10;    SELECT &#13;&#10;        i.idioma_id,&#13;&#10;        i.nombre&#13;&#10;    FROM idioma i&#13;&#10;    WHERE i.idioma_id = &apos;37C99BDE-5A59-48E2-96D3-971D578F4815&apos;&#13;&#10;    AND NOT EXISTS (SELECT 1 FROM SesionIdioma);&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerIdiomas]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerIdiomas]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT idioma_id, nombre&#13;&#10;    FROM Idioma&#13;&#10;    ORDER BY idioma_id ASC;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerIdiomasActivos]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerIdiomasActivos]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        idioma_id,&#13;&#10;        nombre,&#13;&#10;        activo&#13;&#10;    FROM Idioma&#13;&#10;    WHERE activo = 1;&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerIdiomasInactivos]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerIdiomasInactivos]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT &#13;&#10;        idioma_id,&#13;&#10;        nombre,&#13;&#10;        activo&#13;&#10;    FROM Idioma&#13;&#10;    WHERE activo = 0;  -- Solo los inactivos&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerPermisosUsuario]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerPermisosUsuario]&#13;&#10;    @id_usuario UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;  &#9;select p.permiso_id AS permiso_id,p.nombre as nombre,p.descripcion as descripcion from usuario_permisos u&#13;&#10;&#9;inner join permisos p on p.permiso_id = u.permiso_id&#13;&#10;&#9;where usuario_id = @id_usuario&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerPrioridad]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;Create PROCEDURE [sp_ObtenerPrioridad]&#13;&#10;    @CategoriaId INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        p.prioridad_id,&#13;&#10;        p.nombre,&#13;&#10;        p.descripcion&#13;&#10;    FROM categoria c&#13;&#10;    INNER JOIN prioridad p ON c.prioridad_id = p.prioridad_id&#13;&#10;    WHERE c.categoria_id = @CategoriaId;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerPrioridadPorId]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;&#13;&#10;CREATE PROCEDURE [sp_ObtenerPrioridadPorId]&#13;&#10;    @PrioridadId INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT&#13;&#10;        prioridad_id,&#13;&#10;        nombre,&#13;&#10;        descripcion&#13;&#10;    FROM Prioridad&#13;&#10;    WHERE prioridad_id = @PrioridadId;&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerRolesUsuario]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerRolesUsuario]&#13;&#10;    @UsuarioId UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        r.rol_id,&#13;&#10;        r.nombre AS rol_nombre, r.descripcion&#13;&#10;    FROM &#13;&#10;        usuario_roles ur&#13;&#10;    INNER JOIN &#13;&#10;        rol r ON ur.rol_id = r.rol_id&#13;&#10;    WHERE &#13;&#10;        ur.usuario_id = @UsuarioId;&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerTecnicoPorId]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE   PROCEDURE [sp_ObtenerTecnicoPorId]&#13;&#10;    @tecnico_id INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT&#13;&#10;        t.tecnico_id,&#13;&#10;        t.usuario_id,&#13;&#10;        u.nombre,&#13;&#10;        u.apellido,&#13;&#10;        u.email,&#13;&#10;        t.activo,&#13;&#10;        t.fecha_alta&#13;&#10;    FROM dbo.Tecnico AS t&#13;&#10;    INNER JOIN dbo.Usuario AS u&#13;&#10;        ON t.usuario_id = u.usuario_id&#13;&#10;    WHERE t.tecnico_id = @tecnico_id;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerTicketPorId]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;&#13;&#10;CREATE PROCEDURE [sp_ObtenerTicketPorId]&#13;&#10;    @ticket_id UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT&#13;&#10;        ticket_id,&#13;&#10;        fecha_creacion,&#13;&#10;        fecha_ultima_modif,&#13;&#10;        fecha_cierre,&#13;&#10;        eliminado,&#13;&#10;        asunto,&#13;&#10;        descripcion,&#13;&#10;        cliente_creador_id,&#13;&#10;        categoria_id,&#13;&#10;        prioridad_id,&#13;&#10;        estado_id,&#13;&#10;        usuario_aprobador_id,&#13;&#10;        grupo_tecnico_id,&#13;&#10;        tecnico_id&#13;&#10;    FROM dbo.Ticket&#13;&#10;    WHERE ticket_id = @ticket_id;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerTodasLasEtiquetas]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerTodasLasEtiquetas]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT etiqueta_id, nombre&#13;&#10;    FROM etiquetas;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerTodosLosIdiomas]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerTodosLosIdiomas]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT &#13;&#10;        idioma_id,&#13;&#10;        nombre,&#13;&#10;        activo&#13;&#10;    FROM Idioma;  -- Sin WHERE, para traer todos (activos + inactivos)&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerTodosTecnicos]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE   PROCEDURE [sp_ObtenerTodosTecnicos]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT&#13;&#10;        t.tecnico_id,&#13;&#10;        t.usuario_id,&#13;&#10;        u.nombre,&#13;&#10;        u.apellido,&#13;&#10;        u.email,&#13;&#10;        t.activo,&#13;&#10;        t.fecha_alta&#13;&#10;    FROM dbo.Tecnico AS t&#13;&#10;    INNER JOIN dbo.Usuario AS u&#13;&#10;        ON t.usuario_id = u.usuario_id&#13;&#10;    ORDER BY t.tecnico_id;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerTraducciones]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerTraducciones]&#13;&#10;    @IdiomaId UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT &#13;&#10;        t.traduccion_id, &#13;&#10;        t.idioma_id, &#13;&#10;        t.etiqueta_id, &#13;&#10;        t.texto, &#13;&#10;        e.nombre AS etiqueta,&#13;&#10;&#9;&#9;e.form as formulario &#13;&#10;    FROM &#13;&#10;        traducciones t&#13;&#10;    INNER JOIN &#13;&#10;        etiquetas e ON t.etiqueta_id = e.etiqueta_id&#13;&#10;    WHERE &#13;&#10;        t.idioma_id = @IdiomaId;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ObtenerTraduccionesPorIdioma]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ObtenerTraduccionesPorIdioma]&#13;&#10;@idioma_id UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        e.etiqueta_id,&#13;&#10;        e.nombre AS EtiquetaNombre,&#13;&#10;        e.form   AS Formulario,&#13;&#10;        e.texto  AS TextoOriginal,&#13;&#10;&#13;&#10;        t.traduccion_id,&#13;&#10;        t.texto       AS TextoTraduccion,&#13;&#10;        t.idioma_id   AS IdiomaId&#13;&#10;&#13;&#10;    FROM etiquetas e&#13;&#10;    LEFT JOIN traducciones t &#13;&#10;        ON t.etiqueta_id = e.etiqueta_id&#13;&#10;       AND t.idioma_id   = @idioma_id&#13;&#10;    ORDER BY e.nombre;&#13;&#10;END;&#13;&#10;&#13;&#10;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerUsuarioDeSesion]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerUsuarioDeSesion]&#13;&#10;    @UsuarioID UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    -- Consulta para obtener el último rol del usuario&#13;&#10;    SELECT TOP 1 ultimo_rol_id&#13;&#10;    FROM sesion&#13;&#10;    WHERE usuario_id = @UsuarioID&#13;&#10;    ORDER BY fecha_inicio DESC;&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerUsuarioIdPorClienteId]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerUsuarioIdPorClienteId]&#13;&#10;    @ClienteID INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        usuario_id&#13;&#10;    FROM &#13;&#10;        Cliente&#13;&#10;    WHERE &#13;&#10;        cliente_id = @ClienteID;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerUsuarioPorId]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerUsuarioPorId]&#13;&#10;    @usuario_id UNIQUEIDENTIFIER&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        u.usuario_id,&#13;&#10;        u.email,&#13;&#10;        u.password,&#13;&#10;        u.nombre,&#13;&#10;        u.apellido,&#13;&#10;        u.nombre_usuario,&#13;&#10;        u.legajo,&#13;&#10;        u.fecha_alta,&#13;&#10;        u.ultimo_inicio_sesion,&#13;&#10;        u.idioma_id,&#13;&#10;        i.nombre AS nombre_idioma&#13;&#10;    FROM usuario u&#13;&#10;    LEFT JOIN idioma i ON i.idioma_id = u.idioma_id&#13;&#10;    WHERE u.usuario_id = @usuario_id;&#13;&#10;END;&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerUsuariosDisponiblesParaAdministrador]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;&#13;&#10;CREATE PROCEDURE [sp_ObtenerUsuariosDisponiblesParaAdministrador]&#13;&#10;    @rol_Admin INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT u.usuario_id, u.nombre, u.apellido, u.email&#13;&#10;    FROM Usuario u&#13;&#10;    INNER JOIN usuario_roles ur ON u.usuario_id = ur.usuario_id&#13;&#10;    LEFT JOIN Administrador a ON u.usuario_id = a.usuario_id&#13;&#10;    WHERE ur.rol_id = @rol_Admin AND a.usuario_id IS NULL;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerUsuariosDisponiblesParaCliente]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerUsuariosDisponiblesParaCliente]&#13;&#10;    @rol_cliente INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT u.usuario_id, u.nombre, u.apellido, u.email&#13;&#10;    FROM Usuario u&#13;&#10;    INNER JOIN usuario_roles ur ON u.usuario_id = ur.usuario_id&#13;&#10;    WHERE ur.rol_id = @rol_cliente&#13;&#10;      AND NOT EXISTS (&#13;&#10;          SELECT 1&#13;&#10;          FROM Cliente c&#13;&#10;          WHERE c.usuario_id = u.usuario_id&#13;&#10;      )&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_ObtenerUsuariosDisponiblesParaTecnico]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_ObtenerUsuariosDisponiblesParaTecnico]&#13;&#10;    @rol_tecnico INT&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT u.usuario_id, u.nombre, u.apellido, u.email&#13;&#10;    FROM Usuario u&#13;&#10;    INNER JOIN usuario_roles ur ON u.usuario_id = ur.usuario_id&#13;&#10;    LEFT JOIN Tecnico t ON u.usuario_id = t.usuario_id&#13;&#10;    WHERE ur.rol_id = @rol_tecnico AND t.usuario_id IS NULL;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_permiso_listar]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;&#13;&#10;CREATE PROCEDURE [sp_permiso_listar]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SELECT nombre,permiso_id,descripcion FROM permisos WHERE  descripcion is null;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_permiso_listar_familias]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_permiso_listar_familias]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT &#13;&#10;        p.permiso_id,&#13;&#10;        p.nombre,&#13;&#10;        p.descripcion&#13;&#10;    FROM &#13;&#10;        permisos p&#13;&#10;    WHERE &#13;&#10;        p.permiso_id NOT IN (&#13;&#10;            SELECT permiso_hijo_id FROM permiso_permisos&#13;&#10;        );&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_permiso_listar_patentes]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_permiso_listar_patentes]&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    SELECT DISTINCT &#13;&#10;        p.permiso_id,&#13;&#10;        p.nombre,&#13;&#10;        p.descripcion&#13;&#10;    FROM &#13;&#10;        permisos p&#13;&#10;        INNER JOIN permiso_permisos pp ON p.permiso_id = pp.permiso_hijo_id;&#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [sp_RegistrarSesion]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [sp_RegistrarSesion]&#13;&#10;    @SessionID uniqueidentifier,&#13;&#10;    @UsuarioID uniqueidentifier,&#13;&#10;    @FechaInicio datetime,&#13;&#10;    @UltimoIdioma uniqueidentifier,&#13;&#10;    @UltimoRolID int,&#13;&#10;    @Estado bit&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;    &#13;&#10;    -- Insert a new session record&#13;&#10;    INSERT INTO sesion (&#13;&#10;        session_id,&#13;&#10;        usuario_id,&#13;&#10;        fecha_inicio,&#13;&#10;        ultimo_idioma,&#13;&#10;        ultimo_rol_id,&#13;&#10;        estado,&#13;&#10;        fecha_fin&#13;&#10;    )&#13;&#10;    VALUES (&#13;&#10;        @SessionID,&#13;&#10;        @UsuarioID,&#13;&#10;        @FechaInicio,&#13;&#10;        @UltimoIdioma,&#13;&#10;        @UltimoRolID,&#13;&#10;        @Estado,&#13;&#10;        NULL -- fecha_fin is NULL for a new session&#13;&#10;    );&#13;&#10;    &#13;&#10;END&#13;&#10;GO&#13;&#10;/****** Object:  StoredProcedure [SP_ValidarEtiquetaExistente]    Script Date: 11/6/2025 19:42:54 ******/&#13;&#10;SET ANSI_NULLS ON&#13;&#10;GO&#13;&#10;SET QUOTED_IDENTIFIER ON&#13;&#10;GO&#13;&#10;CREATE PROCEDURE [SP_ValidarEtiquetaExistente]&#13;&#10;    @Nombre NVARCHAR(255),&#13;&#10;    @Form NVARCHAR(255),&#13;&#10;    @Texto NVARCHAR(MAX)&#13;&#10;AS&#13;&#10;BEGIN&#13;&#10;    SET NOCOUNT ON;&#13;&#10;&#13;&#10;    IF EXISTS (&#13;&#10;        SELECT 1&#13;&#10;        FROM etiquetas&#13;&#10;        WHERE nombre = @Nombre&#13;&#10;        AND form = @Form&#13;&#10;        AND texto = @Texto&#13;&#10;    )&#13;&#10;    BEGIN&#13;&#10;        SELECT 1 AS Existe; -- Existe la etiqueta&#13;&#10;    END&#13;&#10;    ELSE&#13;&#10;    BEGIN&#13;&#10;        SELECT 0 AS Existe; -- No existe la etiqueta&#13;&#10;    END&#13;&#10;END;&#13;&#10;GO&#13;&#10;USE [master]&#13;&#10;GO&#13;&#10;ALTER DATABASE [CRM_TEST] SET  READ_WRITE &#13;&#10;GO&#13;&#10;" Flags="18" Order="0"/>
